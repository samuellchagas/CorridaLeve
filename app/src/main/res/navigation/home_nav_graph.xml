<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/historicFragment2"
        android:name="com.example.corridaleve.fragments.HistoricFragment"
        android:label="HistoricFragment"
        tools:layout="@layout/historic_fragment"/>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.corridaleve.fragments.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_homeFragment_to_screenRunActivity"
            app:destination="@id/screenRunActivity" />
    </fragment>
    <activity
        android:id="@+id/screenRunActivity"
        android:name="com.example.corridaleve.activities.ScreenRunActivity"
        android:label="activity_screen_run"
        tools:layout="@layout/activity_screen_run" />
</navigation>